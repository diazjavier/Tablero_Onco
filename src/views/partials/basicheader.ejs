      <!-- Brand Logo -->
      <a href="index.html" class="brand-link">
        <div class="sidebar-brand-icon rotate-n-15 d-flex justify-content-center">
          <i class="fas fa-2x fa-pills"></i>
        </div>
        <div class="sidebar-brand-text mx-3 text-center">
          <p class="brand">Control de <br>Medicamentos</p>
        </div>
      </a>

      <!-- Sidebar -->
      <div class="sidebar">

        <!-- Sidebar Menu -->
        <nav class="mt-2">
          <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
            <!-- Add icons to the links using the .nav-icon class
               with font-awesome or any other icon font library -->
            <!-- Divider -->
            <hr class="sidebar-divider my-0">


            <li class="nav-item mt-2">
              <a href="periodo" class="nav-link">
                <i class="nav-icon fas fa-calendar-alt"></i>
                <p>
                  Período
                </p>
              </a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">


            <li class="nav-item mt-2">
              <a href="evolucion" class="nav-link">
                <i class="nav-icon fas fa-chart-line"></i>
                <p>
                  Evolución del contrato
                </p>
              </a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">


            <li class="nav-item mt-2">
              <a href="precios" class="nav-link">
                <i class="nav-icon fas fa-dollar-sign"></i>
                <p>Precios</p>
              </a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">


            <li class="nav-item mt-2">
              <a href="consumo" class="nav-link">
                <i class="nav-icon fas fa-tablets"></i>
                <p>Consumo</p>
              </a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">


            <li class="nav-item mt-2">
              <a href="dashboard" class="nav-link">
                <i class="nav-icon fas fa-chart-area"></i>
                <p>
                  Dashboard
                </p>
              </a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">


            <li class="nav-item mt-2">
              <a href="genericos" class="nav-link">
                <i class="nav-icon fas fa-flask"></i>
                <p>
                  Genericos
                </p>
              </a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">


            <li class="nav-item mt-2">
              <a href="comerciales" class="nav-link">
                <i class="nav-icon fas fa-prescription"></i>
                <p>
                  Comerciales
                </p>
              </a>
            </li>
            <% if(elUsuario.idRol == 2){ %>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">

            <li class="nav-item mt-2">
              <a href="usuarios" class="nav-link">
                <i class="nav-icon fas fa-users"></i>
                <p>
                  Usuarios
                </p>
              </a>
            </li>
            <% } %>
            <!-- Divider -->
            <hr class="sidebar-divider my-0">

          </ul>
        </nav>
